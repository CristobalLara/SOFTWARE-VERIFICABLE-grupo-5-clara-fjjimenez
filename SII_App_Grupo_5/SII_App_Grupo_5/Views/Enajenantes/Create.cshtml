@model SII_App_Grupo_5.Models.Enajenante

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Enajenante</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="InscripcionId" class="control-label"></label>
                <select asp-for="InscripcionId" class ="form-control" asp-items="ViewBag.InscripcionId"></select>
            </div>
            <div class="form-group">
                <label asp-for="Rut" class="control-label"></label>
                <input asp-for="Rut" class="form-control" />
